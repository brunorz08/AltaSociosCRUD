@page
@model             EditarModel
@{
	ViewData["Title"] = "Editar socio";

	
	<form action="" method="post">
		<div>
			<b>
				<span>Nombre:</span>
			</b>
			<br>
			<input name="nombre" type="text" value="@Model.socio.nombre">
		</div>

		<br>

		<div>
			<b><span>Apellido:</span></b>
			<br>
			<input name="apellido" type="text" value="@Model.socio.apellido">
		</div>
		<br>
		<div>
			<b>
				<span>Documento:</span>
			</b>
			<br>
			<input name="documento" type="number" value="@Model.socio.Documento">
			<br>
		</div>
		<br>
		<div>
			<b>
				<span>Celular:</span>
			</b>
			<br>
			<input name="celular" type="number" value="@Model.socio.Celular">
		</div>
		<br>
		<div>
			<b>	<span>Email:</span></b>
			<br>
			<input name="email" type="text" value="@Model.socio.Email">
		</div>
		<br>
		<div>
		</div>
		<br />
		<br />
		<button class="btn btn-outline-success">Enviar</button>
		<br />
		<br />


	</form>

	<a href="Index">
		<button class="btn btn-outline-secondary">Regresar</button>
	</a>

}
